<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.maaaarckyo.donagoandroid.Activities.MenuActivity">

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aktivitet"
        android:id="@+id/btnUActivity"
        android:layout_marginTop="135dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VelgÃ¸renhed"
        android:id="@+id/btnNGO"
        android:layout_alignBottom="@+id/btnUActivity"
        android:layout_toRightOf="@+id/btnUActivity"
        android:layout_toEndOf="@+id/btnUActivity" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Socialt "
        android:id="@+id/btnSocial"
        android:layout_alignTop="@+id/btnNGO"
        android:layout_toRightOf="@+id/btnMyDonation"
        android:layout_toEndOf="@+id/btnMyDonation" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historie"
        android:id="@+id/btnUHistory"
        android:layout_below="@+id/btnUActivity"
        android:layout_toLeftOf="@+id/btnNGO"
        android:layout_toStartOf="@+id/btnNGO"
        android:layout_marginTop="33dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rangliste"
        android:id="@+id/btnURank"
        android:layout_alignTop="@+id/btnMyDonation"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Instillinger"
        android:id="@+id/btnSettings"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/btnUHistory"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profil"
        android:id="@+id/btnUProfile"
        android:layout_alignTop="@+id/btnSettings"
        android:layout_alignLeft="@+id/btnURank"
        android:layout_alignStart="@+id/btnURank" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mine donationer"
        android:id="@+id/btnMyDonation"
        android:layout_above="@+id/btnSettings"
        android:layout_alignLeft="@+id/btnNGO"
        android:layout_alignStart="@+id/btnNGO" />
</RelativeLayout>
